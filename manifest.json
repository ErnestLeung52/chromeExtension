{
  "name": "Regis Coins",
  "description": "Replaces images with photos of regis and accumulates coins!",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "regisIcon.png",
    "default_popup": "popup.html",
    "default_title": "Regis Coins"
  },
  "permissions": ["activeTab"]
}
